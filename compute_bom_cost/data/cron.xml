<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record model="ir.cron" id="compute_cost_cron">
        <field name="name">BOM Cost Compute Cron</field>
        <field name="model_id" ref="compute_bom_cost.model_product_template"/>
        <field name="state">code</field>
        <field name="code">model._cron_auto_compute_cost()</field>
        <field name="active" eval="False"/>
        <field name="user_id" ref="base.user_root"/>
        <field name="interval_number">1</field> <!-- To decide clearly -->
        <field name="interval_type">hours</field>
        <field name="numbercall">-1</field>
    </record>
</odoo>